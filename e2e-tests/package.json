{
  "name": "jira-clone-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for Jira Clone with automatic bug reporting",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:bdd": "cucumber-js --config cucumber.config.js",
    "test:bdd:headed": "HEADLESS=false cucumber-js --config cucumber.config.js",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "install-browsers": "playwright install"
  },
  "dependencies": {
    "@playwright/test": "^1.44.0",
    "dotenv": "^16.0.0",
    "@cucumber/cucumber": "^10.3.1",
    "playwright-cucumber": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@cucumber/typescript": "^1.0.0"
  }
}